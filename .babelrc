{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          { "modules": "umd" }
        ]
      ],
      "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-regenerator"
      ],
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          { "modules": false }
        ]
      ]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "ignore": [
    "node_modules/**"
  ]
}
